{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Skypydb Docs",
  "colors": {
    "primary": "#a6d2ff",
    "light": "#ffe1a6",
    "dark": "#ffe1a6"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting started",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "getting-started/introduction",
              "getting-started/quickstart",
              "getting-started/basic_implementation"
            ]
          }
        ]
      },
      {
        "tab": "API Guide",
        "groups": [
          {
            "group": "API guide",
            "pages": [
              "api-guides/overview"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-guides/endpoint/add",
              "api-guides/endpoint/delete",
              "api-guides/endpoint/get",
              "api-guides/endpoint/generate_key",
              "api-guides/endpoint/encrypt",
              "api-guides/endpoint/custom_config"
            ]
          }
        ]
      },
      {
        "tab": "CLI",
        "groups": [
          {
            "group": " ",
            "pages": [
              "cli/overview"
            ]
          },
          {
            "group": "Commands",
            "pages": [
              "cli/dev"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/Ahen-Studio/skypy-db",
          "icon": "github"
        }
