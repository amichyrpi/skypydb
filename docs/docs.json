{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Skypydb",
  "colors": {
    "primary": "#000",
    "light": "#b5b5b5",
    "dark": "#000"
  },
  "favicon": "/favicon.png",
  "background": {
    "color": {
      "light": "#ffffff",
      "dark": "#000000"
    }
  },
  "fonts": {
    "family": "Inter",
    "heading": {
      "family": "Inter"
    },
    "body": {
      "family": "Inter"
    }
  },
  "styling": {
    "codeblocks": "system"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Stars use on Github",
      "href": "https://github.com/Ahen-Studio/skypy-db"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Docs",
        "icon": "book",
        "pages": [
          {
            "group": "Overview",
            "pages": [
              "getting-started/getting_started"
            ]
          },
        {
          "group": "Reactive Client",
          "pages": [
            "reactiveclient/schema",
            "reactiveclient/add",
            "reactiveclient/delete",
            "reactiveclient/get",
            "reactiveclient/encryption"
          ]
        },
          {
            "group": "Vector Client",
            "pages": [
              "vectorclient/add",
              "vectorclient/query",
              "vectorclient/delete"
            ]
          },
          {
            "group": "Cli",
            "pages": [
              "cli/overview"
            ]
          }
        ]
      },
      {
        "tab": "Integrations",
        "icon": "code-merge",
        "pages": [
          {
            "group": "Mem0 integrations",
            "pages": [
              "integrations/mem0/overview"
            ]
          },
          {
            "group": "Mem0 component",
            "pages": [
              "integrations/mem0/configs",
              "integrations/mem0/utils",
              "integrations/mem0/vector_stores"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/Ahen-Studio/skypy-db",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "errors": {
  "404": {
    "redirect": false,
    "title": "I think you shouldn't be here",
    "description": "What happened to this page?"
  }
},
  "footer": {
    "socials": {
      "github": "https://github.com/Ahen-Studio/skypy-db"
    }
  }
}
