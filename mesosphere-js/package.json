{
  "name": "mesosphere",
  "version": "2.0.0",
  "description": "Open Source Relational and Vector Embeddings Database",
  "license": "Apache",
  "author": "Ahen Studio",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mesosphere": "./dist/cli.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./httpclient": {
      "types": "./dist/httpclient.d.ts",
      "import": "./dist/httpclient.mjs",
      "require": "./dist/httpclient.cjs"
    },
    "./functions": {
      "types": "./dist/functions.d.ts",
      "import": "./dist/functions.mjs",
      "require": "./dist/functions.cjs"
    },
    "./serverside": {
      "types": "./dist/serverside.d.ts",
      "import": "./dist/serverside.mjs",
      "require": "./dist/serverside.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup && node scripts/build_codegen.cjs",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "commander": "^14.0.1",
    "onnxruntime-node": "^1.23.0",
    "openai": "^6.5.0",
    "prompts": "^2.4.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/prompts": "^2.4.9",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "vitest": "^3.2.4"
  }
}
